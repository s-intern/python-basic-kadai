        "# 変数varの値に応じて条件分岐\n",
        "\n",
        "# 変数varを指定\n",
        "var = 15\n",
        "\n",
        "# 条件分岐\n",
        "if var % 3 == 0 and var % 5 != 0:\n",
        "  print(\"Fizz\")\n",
        "elif var % 3 != 0 and var % 5 == 0:\n",
        "  print(\"Buzz\")\n",
        "elif var % 3 == 0 and var %5 == 0:\n",
        "  print(\"FizzBuzz\")\n",
        "else:\n",
        "  print(var)"
      ]
