        "## クラスの利用\n",
        "\n",
        "# クラスの作成\n",
        "class Human:\n",
        "  # 属性\n",
        "  def __init__(self, name, age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "  # メソッド\n",
        "  def printinfo(self):\n",
        "    print(f\"{self.name}さんは{self.age}歳です。\")\n",
        "\n",
        "# インスタンス化\n",
        "user = Human(\"侍太郎\", 30)\n",
        "\n",
        "# 名前と年齢を表示\n",
        "user.printinfo()"
      ]
    }
  ]
